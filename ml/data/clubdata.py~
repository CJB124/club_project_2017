import urllib3
import json

http = urllib3.PoolManager()
site = http.request('GET','https://data.wprdc.org/api/action/datastore_search_sql?sql=SELECT * FROM "40776043-ad00-40f5-9dc8-1fde865ff571" WHERE "NEIGHBORHOOD" LIKE \'%Oakland\' ORDER BY "CREATED_ON" DESC LIMIT 25')

data = json.loads(site.data.decode('utf-8'))

with open('data.txt','w') as out:
    json.dump(data,out)
